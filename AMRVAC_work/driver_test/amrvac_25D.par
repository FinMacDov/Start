! $AMRVAC_DIR/setup.pl -d=23 -phi=0 -z=0 -g=16,16 -p=mhd -eos=default -nf=1 -ndust=0 -u=nul
 &testlist
        teststr=''
 /


 &filelist
        filenamelog='./output_256.0_si/jet_si'
        filenameout='./output_256.0_si/jet_log_si'
        primnames= 'rho v1 v2 v3 p b1 b2 b3 tr'
        saveprim=.true.
        autoconvert=.true.
        nwauxio = 1
        convert_type='vtuBmpi'
/

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave(1)= 1.0
        dtsave(2)=0.2
 /

 &stoplist
        dtmin=1.D-12
        tmax=50
 /

 &methodlist
        wnames=       'rho m1 m2 m3 e b1 b2 b3 tr'
        fileheadout=  'inlet'
        typeadvance=  'ssprk54'
        typefull1=    13*'hllc'
        typepred1=    13*'hllc'
        typelimiter1= 13*'koren'
        typegradlimiter1 = 13*'koren'
        typelimited='predictor'
        useprimitive=.true.
        
        dimsplit=.false.
        fixsmall=.true.
        strictsmall=.false.
        strictgetaux=.True.
        nflatgetaux=2
        smallp=1.0d-5
        smallrho=1.0d-6
	!strictzero = .T.
! MHD
        ssplitdivb=.true.
        typedivbfix='janhunen'
 /

 &boundlist
        dixB=3
!        typeB=9*'cont',9*'cont',9*'cont', 9*'cont'
!        typeB=9*'cont',9*'cont',9*'special', 9*'cont'
        typeB=	9*'noinflow',
	      	9*'noinflow',
		'symm','symm','asymm','symm','symm','symm','symm','symm','symm' 
		9*'cont'
 /

 &amrlist
        !errorestimate=0
        mxnest = 3
        nxlone1=50
        nxlone2=250
        xprobmin1= -2.0d0
        xprobmax1= 2.0d0
        xprobmin2=0.0d0
        xprobmax2=10.0d0
 /

 &paramlist
        courantpar=0.6
        slowsteps=1000
 /
